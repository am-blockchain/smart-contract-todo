{
  "name": "@todo-list/contracts",
  "version": "0.1.0",
  "description": "A Chainlinked Matic NFT enabled Wicher Poker Game",
  "license": "MIT",
  "scripts": {
    "compile": "npx truffle compile",
    "console:dev": "npx truffle console --network devGanache",
    "console:mumbai": "npx truffle console --network mumbaiTestnet",
    "depcheck": "echo '@chainlink/box' && depcheck --ignore-dirs=build/contracts || true",
    "solhint": "solhint ./contracts/**/*.sol",
    "lint": "yarn solhint",
    "migrate:dev": "npx truffle migrate --reset --network devGanache",
    "migrate:mumbai": "npx truffle migrate --network mumbaiTestnet",
    "test": "npx truffle test"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.1.6",
    "@openzeppelin/contracts": "^3.4.0",
    "@truffle/hdwallet-provider": "^1.2.3",
    "truffle-privatekey-provider": "^1.5.0",
    "dotenv": "^8.2.0",
    "cross-env": "7.0.3"
  },
  "devDependencies": {
    "@chainlink/belt": "^0.0.3",
    "@chainlink/test-helpers": "0.0.5",
    "@openzeppelin/test-helpers": "^0.5.10",
    "chai": "^4.3.0",
    "depcheck": "^1.3.1",
    "solhint": "^3.3.2",
    "truffle": "^5.1.65"
  },
  "resolutions": {
    "**/minimist": "^0.2.1",
    "**/node-fetch": "^2.6.1",
    "**/yargs-parser": "^13.1.2",
    "**/mem": "^4.0.0"
  }
}